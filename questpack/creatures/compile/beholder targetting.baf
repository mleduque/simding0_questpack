        GlobalTimerNotExpired("IgnoreBounce","LOCALS")
    OR(2)
        !GlobalTimerNotExpired("IgnoreImmunity","LOCALS") // Don't check anything below here if I'm ignoring immunity
        RandomNumGT(20,10)
    OR(11)
        !See(TenthNearestEnemyOf(Myself))
        Gender(TenthNearestEnemyOf(Myself),SUMMONED)
        !CheckStat(TenthNearestEnemyOf(Myself),0,SANCTUARY)
        HasItemEquiped("CLCK26",TenthNearestEnemyOf(Myself))
        HasItemEquiped("WA2SHIEL",TenthNearestEnemyOf(Myself))
        HasItemEquiped("RING09",TenthNearestEnemyOf(Myself))
        HasItemEquiped("SHLD25",TenthNearestEnemyOf(Myself))
        HasItemEquiped("SPER07",TenthNearestEnemyOf(Myself))
        HasItemEquiped("SW1H27",TenthNearestEnemyOf(Myself))
        Kit(TenthNearestEnemyOf(Myself),INQUISITOR)
        Kit(TenthNearestEnemyOf(Myself),UNDEADHUNTER)
        CheckStatGT(TenthNearestEnemyOf(Myself),50,RESISTMAGIC)
        CheckStatGT(TenthNearestEnemyOf(Myself),0,SAVEVSSPELL)
        CheckStatGT(TenthNearestEnemyOf(Myself),0,CLERIC_FREE_ACTION)
        StateCheck(TenthNearestEnemyOf(Myself),STATE_BERSERK)
        StateCheck(TenthNearestEnemyOf(Myself),STATE_REALLY_DEAD)
        StateCheck(TenthNearestEnemyOf(Myself),D0_STATE_DISABLED)
        GlobalTimerNotExpired("IgnoreImmunity","LOCALS")

    False()
THEN
    RESPONSE #100
        Continue()
END

